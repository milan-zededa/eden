{
  "ports": [
    {
      "logicalLabel": "my-eve0",
      "mac": "06:00:00:00:00:01",
      "adminUP": true,
      "eveConnect": {
        "ifOrder": 0,
        "mac": "06:00:00:00:00:11"
      }
    },
    {
      "logicalLabel": "my-eve1",
      "mac": "06:00:00:00:00:02",
      "adminUP": true,
      "eveConnect": {
        "ifOrder": 0,
        "mac": "06:00:00:00:00:12"
      }
    }
  ],
  "bonds": [
    {
      "logicalLabel": "my-bond0",
      "ports": ["my-eve0", "my-eve1"],
      "mode": "active-backup"
    }
  ],
  "bridges": [
    {
      "logicalLabel": "my-bridge0",
      "bonds": ["my-bond0"]
    }
  ],
  "networks": [
    {
      "logicalLabel": "my-network0",
      "bridge": "my-bridge0",
      "subnet": "172.22.12.0/24",
      "gwIP": "172.22.12.1",
      "dhcp": {
        "enable": true,
        "ipRange": {
          "fromIP": "172.22.12.10",
          "toIP": "172.22.12.100"
        },
        "domainName": "sdn",
        "privateDNS": ["my-dns-server"],
        "publicDNS": ["1.1.1.1"]
      },
      "router": {
        "outsideReachability": true,
        "reachableEndpoints": ["my-client", "my-dns-server"]
      }
    }
  ],
  "endpoints": {
    "clients": [
      {
        "logicalLabel": "my-client",
        "fqdn": "my-client.sdn",
        "subnet": "10.15.15.0/24",
        "ip": "10.15.15.10"
      }
    ],
    "dnsServers": [
      {
        "logicalLabel": "my-dns-server",
        "fqdn": "my-dns-server.sdn",
        "subnet": "10.16.16.0/24",
        "ip": "10.16.16.25",
        "staticEntries": [
          {
            "fqdn": "mydomain.adam",
            "ip": "adam-ip"
          }
        ]
      }
    ]
  }
}