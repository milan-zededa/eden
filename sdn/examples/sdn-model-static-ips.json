{
  "ports": [
    {
      "logicalLabel": "eveport",
      "adminUP": true
    }
  ],
  "bridges": [
    {
      "logicalLabel": "bridge",
      "ports": ["eveport"]
    }
  ],
  "networks": [
    {
      "logicalLabel": "network",
      "bridge": "bridge",
      "subnet": "172.22.12.0/24",
      "gwIP": "172.22.12.1",
      "dhcp": {
        "enable": false
      },
      "router": {
        "outsideReachability": true,
        "reachableEndpoints": ["my-client", "my-dns-server"]
      }
    }
  ],
  "endpoints": {
    "clients": [
      {
        "logicalLabel": "my-client",
        "fqdn": "my-client.sdn",
        "subnet": "10.15.15.0/24",
        "ip": "10.15.15.10"
      }
    ],
    "dnsServers": [
      {
        "logicalLabel": "my-dns-server",
        "fqdn": "my-dns-server.sdn",
        "subnet": "10.16.16.0/24",
        "ip": "10.16.16.25",
        "staticEntries": [
          {
            "fqdn": "mydomain.adam",
            "ip": "adam-ip"
          }
        ]
      }
    ]
  }
}